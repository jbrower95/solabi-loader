{
    "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "noImplicitAny": false,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "allowJs": true,
      "outDir": "dist"
      
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules/**/*"],
}